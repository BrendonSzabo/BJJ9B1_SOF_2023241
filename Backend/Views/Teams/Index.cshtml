                @Html.DisplayFor(modelItem => item.TopId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JungleId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MidId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ADCId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupportId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
