            <th>
                @Html.DisplayNameFor(model => model.JungleId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MidId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ADCId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SupportId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
